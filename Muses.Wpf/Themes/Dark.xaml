<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#FF2D2D30</Color>
    <Color x:Key="ForegroundColor">#FFFFFFFF</Color>
    
    <Color x:Key="BorderColor">#FF3F3F46</Color>
    
    <Color x:Key="DisabledBackColor">#FF2D2D30</Color>
    <Color x:Key="DisabledColor">#FF3F3F46</Color>
    
    <Color x:Key="ControlColor">#FF333337</Color>
    <Color x:Key="ControlHoveredColor">#FF3F3F46</Color>
    <Color x:Key="ControlFocusColor">#FF333337</Color>
    
