    <Color x:Key="AccentLightColor">#FF52B0EF</Color>
    <Color x:Key="AccentDarkColor">#FF035083</Color>
    <Color x:Key="AccentHoverColor">#FF1C86CD</Color>
    <Color x:Key="AccentForegroundColor">#FFFFFFFF</Color>
    
    <Color x:Key="InactiveColor">#FF3F3F46</Color>
    <Color x:Key="InactiveLightColor">#FF555555</Color>
    <Color x:Key="InactiveLightTextColor">#FF999999</Color>

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Muses.Wpf;component/Resources/Brushes.xaml" />
